{
  "workbench.colorTheme": "Default Dark+",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "redhat.telemetry.enabled": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnType": true
  },
  "java.help.firstView": "gettingStarted",
  "tabnine.experimentalAutoImports": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.languageServer": "Pylance",
  "diffEditor.ignoreTrimWhitespace": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.iconTheme": "vscode-icons",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cSpell.userWords": [
    "anki",
    "cloze",
    "clozify",
    "Fung",
    "IYCN",
    "PLSQL",
    "templater"
  ],
  "cSpell.caseSensitive": true,
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "cSpell.enableFiletypes": ["!cpp"],
  "cmake.configureOnOpen": true,
  "eslint.format.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "git.openRepositoryInParentFolders": "never",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.defaultInterpreterPath": "/usr/bin",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "cpp": true
  },
  "C_Cpp.default.cppStandard": "c++17",
  "trunk.trunkGrayOutNonBlockingIssues": false,
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "editor.defaultFormatter": "trunk.io",
  "editor.minimap.enabled": false,
  "trunk.showPreexistingIssues": false,
  "flake8.args": ["--max-line-length=100"],
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "mypy-type-checker.args": ["--namespace-packages"],
  "python.analysis.diagnosticMode": "workspace",
  "sqlite.databaseExtensions": ["db", "db3", "sdb", "s3db", "sqlite3"],
  "workbench.editor.defaultBinaryEditor": "sqlite-viewer.view",
  "files.autoSave": "afterDelay"
}
